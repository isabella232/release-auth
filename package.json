    "deploy": "now -e CLIENT_ID=@client-id -e CLIENT_SECRET=@client-secret --alias release-auth.now.sh"
  },
  "repository": "zeit/release-auth",
  "license": "MIT",
  "dependencies": {
    "micro": "^6.1.0",
    "request": "^2.79.0",
    "request-promise-native": "^1.0.3"
  }
}
